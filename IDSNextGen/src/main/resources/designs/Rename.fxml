<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane id="AnchorPane" prefHeight="300.0" prefWidth="450.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.102" fx:controller="com.agnisys.idsnextgen.controllers.RenameController">
    <stylesheets>
        <URL value="@/styles/rename.css" />
    </stylesheets>
     <top>
      <Pane prefHeight="48.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblWizardTitle" layoutX="14.0" layoutY="14.0">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </top>
    <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <children>
            <Label fx:id="lblFileType" layoutX="17.0" layoutY="44.0" />            
            <TextField fx:id="txtChangeName" layoutX="112.0" layoutY="40.0" prefHeight="25.0" prefWidth="326.0" />
         </children>
      </Pane>
   </center>
    <bottom>
      <Pane prefHeight="44.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnOk" layoutX="330.0" layoutY="10.0" mnemonicParsing="false" onAction="#click_ok" prefHeight="25.0" prefWidth="52.0" text="OK" />
            <Button fx:id="btnCancel" layoutX="392.0" layoutY="10.0" mnemonicParsing="false" onAction="#closeButtonAction" text="Cancel" />
         </children>
      </Pane>
   </bottom>
</BorderPane>
