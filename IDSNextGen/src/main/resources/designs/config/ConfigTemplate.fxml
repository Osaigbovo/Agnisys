<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainChildContainer" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.102" fx:controller="com.agnisys.idsnextgen.controllers.config.ConfigTemplateController">
    <stylesheets>
        <URL value="@../../styles/configmain.css" />
    </stylesheets>
    <children>
      <Label fx:id="lblGeneral" layoutX="24.0" layoutY="14.0" styleClass="heading" text="Settings &gt; Template">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <CheckBox fx:id="chkHideProp" layoutX="82.0" layoutY="70.0" mnemonicParsing="false" text="Hide Template Properties" />
      <CheckBox fx:id="chkHideDesc" layoutX="82.0" layoutY="115.0" mnemonicParsing="false" text="Hide Template Description" />
      
   </children>
</AnchorPane>
