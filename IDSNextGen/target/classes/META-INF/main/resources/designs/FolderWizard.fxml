<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<BorderPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.agnisys.idsnextgen.controllers.FolderController">
     <top>
      <Pane prefHeight="48.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblNewProj" layoutX="14.0" layoutY="14.0">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </top>
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblProjName" layoutX="40.0" layoutY="14.0" />
            <Label fx:id="lblProjLoc" layoutX="40.0" layoutY="47.0" />
            <TextField fx:id="txtProjName" layoutX="151.0" layoutY="10.0" prefHeight="25.0" prefWidth="351.0" />
            <TextField fx:id="txtProjLoc" layoutX="151.0" layoutY="43.0" prefHeight="25.0" prefWidth="351.0" />
            <Button fx:id="btnBrowse" layoutX="516.0" layoutY="43.0" mnemonicParsing="false" onAction="#browseProjLoc" />
         </children>
      </Pane>
   </center>
   <bottom>
      <Pane prefHeight="44.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnCreateProj" layoutX="457.0" layoutY="10.0" mnemonicParsing="false" onAction="#click_createFolder" />
            <Button fx:id="btnCancel" layoutX="519.0" layoutY="10.0" mnemonicParsing="false" onAction="#closeButtonAction" />
         </children>
      </Pane>
   </bottom>
</BorderPane>
